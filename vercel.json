{
    "version" : 2,
    "builds": [
        {
            "src": "./dist/src/app.js",
            "use": "@vercel/node",
            "config" : {"includeFiles":["prisma/schema.prisma"]}
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/dist/src/app.js"
        }
    ],
    "buildCommand": "npm run build"
}